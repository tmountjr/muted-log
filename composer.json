{
    "name": "tmountjr/muted-log",
    "description": "Provides a \"muted\" log writer. The primary purpose is to log exceptions without including parameters in the stack trace.",
    "keywords": ["laravel", "monolog", "exception"],
    "homepage": "https://github.com/tmountjr/muted-log",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Nelson",
            "email": "patnelson@ebay.com",
            "role": "developer"
        },
        {
            "name": "Tom Mount",
            "email": "tmountjr@ebay.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/log": "~4.1"
    },
    "autoload": {
        "classmap": [
            "src/models"
        ],
        "psr-0": {
            "Tmountjr\\MutedLog": "src/"
        }
    },
    "minimum-stability": "dev"
}
